{
  "version": 2,
  "functions": {
    "src/server/index.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/index.css",
      "dest": "/public/index.css"
    },
    {
      "src": "/loading.gif",
      "dest": "/public/loading.gif"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
